<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@ninza.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ChallanDetailController">
   <children>
      <BorderPane layoutX="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <HBox alignment="TOP_CENTER" prefHeight="400.0" BorderPane.alignment="CENTER">
               <children>
                  <TableView fx:id="challandetailtableview" prefHeight="400.0" prefWidth="975.0" HBox.hgrow="ALWAYS">
                    <columns>
                      <TableColumn fx:id="referchallanidcolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Refer Challan ID" />
                      <TableColumn fx:id="challanidcolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Challan ID" />
                        <TableColumn fx:id="assigneenamecolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Assignee Name" />
                        <TableColumn fx:id="productidcolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Product ID" />
                        <TableColumn fx:id="issuecolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text=" Qty Issued" />
                        <TableColumn fx:id="receivecolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Qty Received" />
                        <TableColumn fx:id="paidcolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Qty Paid" />
                        <TableColumn fx:id="billdatecolumn" maxWidth="150.0" minWidth="80.0" prefWidth="120.0" text="Bill Date" />
                    </columns>
                  </TableView>
               </children>
            </HBox>
         </center>
         <bottom>
            <HBox alignment="CENTER" prefHeight="50.0" style="-fx-background-color: #222222;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="closebutton" alignment="CENTER" mnemonicParsing="false" onAction="#closeScreen" prefHeight="24.0" prefWidth="64.0" styleClass="button_left_blackBG" stylesheets="@ninza.css" text="Close" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets top="20.0" />
               </BorderPane.margin>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
